{
  "manifest_version": 3,
  "name": "YT-DLP Downloader",
  "version": "1.0",
  "description": "Add a download button next to YouTube subscribe button.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon.png"
  }
}
